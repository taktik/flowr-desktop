{
  "name": "flowr-desktop",
  "version": "0.2.2",
  "author": {
    "email": "hj@taktik.com",
    "name": "Hubert Jacquin"
  },
  "description": "A PC client for Flowr",
  "keywords": [
    "web-browser",
    "material",
    "electron",
    "react",
    "mobx",
    "styled-components"
  ],
  "main": "build/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taktik/wexond.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV='dev' node fuse.js",
    "build": "node fuse.js",
    "setup": "npm install && npm run install-app-deps && npm run install-widevine && npm run copy-header && npm run build",
    "setup-win": "npm install && npm run install-app-deps && npm run install-widevine && npm run copy-header-win && npm run build",
    "start": "cross-env ENV='dev' electron . --inspect=3001",
    "compile-win32": "electron-builder -w",
    "compile-darwin": "electron-builder -m",
    "compile-linux": "electron-builder -l",
    "lint": "tslint \"src/**/*.ts*\" --exclude \"src/wexond/**/*.ts*\" --project tsconfig.json",
    "lint-fix": "prettier --write \"src/**/*.ts*\" && tslint \"src/**/*.ts*\" --fix",
    "install-app-deps": "electron-builder install-app-deps",
    "install-widevine": "npm install castlabs/electron-releases#v5.0.3-wvvmp --no-save",
    "copy-header": "cp -r $HOME/.electron-gyp/5.0.3 $HOME/.electron-gyp/5.0.3-wvvmp",
    "copy-header-win": "cp -r %HOME%/.electron-gyp/5.0.3 %HOME%/.electron-gyp/5.0.3-wvvmp"
  },
  "pre-commit": [
    "lint-fix"
  ],
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/chrome": "0.0.83",
    "@types/fs-extra": "^7.0.0",
    "@types/gsap": "1.20.2",
    "@types/leveldown": "4.0.0",
    "@types/levelup": "3.1.0",
    "@types/lodash": "^4.14.132",
    "@types/nedb": "1.8.7",
    "@types/node": "12.0.0",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "@types/styled-components": "4.1.14",
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "electron": "5.0.3",
    "electron-builder": "20.43.0",
    "fuse-box": "3.7.1",
    "husky": "3.0.5",
    "prettier": "1.17.0",
    "terser": "3.17.0",
    "tslint": "5.16.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.3.3",
    "typescript-plugin-styled-components": "1.4.2"
  },
  "dependencies": {
    "@cliqz/adblocker": "0.9.1",
    "@fortawesome/fontawesome-svg-core": "1.2.21",
    "@fortawesome/free-solid-svg-icons": "5.10.1",
    "@fortawesome/react-fontawesome": "0.1.4",
    "axios": "0.18.0",
    "deep-extend": "^0.6.0",
    "electron-log": "3.0.5",
    "electron-updater": "4.0.6",
    "ffmpeg-static": "^2.4.0",
    "ffprobe": "^1.1.0",
    "ffprobe-static": "^3.0.0",
    "file-type": "11.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^8.0.1",
    "gsap": "2.1.2",
    "icojs": "0.12.3",
    "leveldown": "5.0.3",
    "levelup": "4.0.1",
    "lodash": "^4.17.11",
    "mobx": "5.9.4",
    "mobx-react": "5.4.3",
    "moment": "^2.24.0",
    "mouse-hooks": "0.3.1",
    "nedb": "1.8.0",
    "network": "^0.4.1",
    "node-vibrant": "3.2.0-alpha",
    "node-window-manager": "1.1.0",
    "pretty-bytes": "5.2.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "run-parallel": "^1.1.9",
    "styled-components": "4.2.0",
    "tldts": "4.0.6",
    "typescript-state-machine": "^0.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
